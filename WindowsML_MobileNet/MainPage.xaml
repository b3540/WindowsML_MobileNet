<Page
    x:Class="WindowsML_MobileNet.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WindowsML_MobileNet"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Loaded="Page_Loaded">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <CaptureElement x:Name="capture" HorizontalAlignment="Center" Height="240" VerticalAlignment="Top" Width="320" Margin="0,20,0,0"/>
        <TextBlock x:Name="statusTBlock" HorizontalAlignment="Center" Margin="0,277,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="320" Height="184"/>
        <TextBlock x:Name="fpsTBlock" HorizontalAlignment="Left" Margin="95,20,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" FontSize="20"/>
    </Grid>
</Page>
